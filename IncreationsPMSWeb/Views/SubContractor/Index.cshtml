@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";


}
<script>
    function Create() {
        var url = '@Url.Action("Create")';
        location.href = url;
    }
    function Exit() {
        var url = '@Url.Action("Index","Home")';
        location.href = url;
    }

    function Open(id) {
        var url = '@Url.Action("Edit")';
        url += "?id=" + id;
        //alert(id);
        window.location = url;
    }
    function Delete(id) {

        var url = '@Url.Action("Delete")';
        location.href = url;
    }
    function Update(id) {

        var url = '@Url.Action("Update")';
        location.href = url;
    }
